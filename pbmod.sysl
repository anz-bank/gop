pbmod:
    @package = "pbmod"
    !type retrieveResponse:
        content <: sequence of keyValue
    !type keyValue:
        key <: string
        value <: string
    !alias str: string
    /resources/{resource <: string}/{version <: string}:
        GET:
            return ok <: retrieveResponse
    /resource:
        GET ?resource=string&version=string:
            return ok <: keyValue